{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [false],
        "quotemark": [true, "single"],
        "no-debugger": false,
        "member-ordering": false,
        "semicolon": [true, "never"],
        "interface-name": [true, "never-prefix"],
        "trailing-comma": true,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
